{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "interface-name": [
      false,
      "never-prefix"
    ],
    "ordered-imports": [
      false
    ],
    "no-console": false,
    "jsx-no-lambda": false,
    "object-literal-sort-keys": false
  },
  "jsRules": {
    "object-literal-sort-keys": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  }
}